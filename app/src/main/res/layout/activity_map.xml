        android:layout_height="match_parent"
        mapbox:mapbox_cameraTargetLat="-7.779483"
        mapbox:mapbox_cameraTargetLng="110.415813"
        mapbox:mapbox_cameraZoom="11" >


    </com.mapbox.mapboxsdk.maps.MapView>

